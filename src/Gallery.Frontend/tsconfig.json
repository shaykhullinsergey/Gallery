{
	"compilerOptions": {
		"allowUnreachableCode": false,
		"allowUnusedLabels": false,
		"sourceMap": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"moduleResolution": "node",
		"jsx": "react",
		"lib": [
			"es6",
			"es7",
			"dom"
		],
		"baseUrl": "./src",
		"paths": {
			"controls": [
				"Controls"
			],
			"controls/*": [
				"Controls/*"
			],
			"pages/*": [
				"Pages/*"
			],
			"pages": [
				"Pages"
			],
			"hubs/*": [
				"Hubs/*"
			],
			"hubs": [
				"Hubs"
			],
			"models/*": [
				"Models/*"
			],
			"models": [
				"Models"
			]
		}
	}
}